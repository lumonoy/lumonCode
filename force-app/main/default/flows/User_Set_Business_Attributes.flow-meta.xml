                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Profit_Center_User_Mapping</targetReference>
            </connector>
            <label>AD Profit Center Updated</label>
        </rules>
    </decisions>
