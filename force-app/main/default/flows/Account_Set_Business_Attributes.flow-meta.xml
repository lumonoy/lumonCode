    <formulas>
        <name>fxBillingAddress</name>
        <dataType>String</dataType>
        <expression>TEXT({!$Record.BillingCountryCode}) 
+ TEXT({!$Record.BillingStateCode})
+ {!$Record.BillingCity}
+ {!$Record.BillingPostalCode}
+ {!$Record.BillingStreet}
+ TEXT({!$Record.BillingLatitude})
+ TEXT({!$Record.BillingLongitude})</expression>
