                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>Get_Postal_Code_Area.District__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Postal_Code_Area__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
