                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>Get_Postal_Code_Area.Id</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Office_and_Profit_Center</targetReference>
            </connector>
