    <formulas>
        <name>fxEmailSignature</name>
        <dataType>String</dataType>
        <expression>CASE({!$Record.Country},
&quot;Spain&quot;, {!$Record.FirstName} &amp; &quot; &quot; &amp; {!$Record.LastName} &amp; BR() &amp; {!$Record.Title} &amp; BR() &amp; {!$Record.MobilePhone} &amp; BR() &amp; {!$Record.Email},
{!$Record.Signature}
)</expression>
    </formulas>
    <formulas>
        <name>fxProfitCenterString</name>
        <dataType>String</dataType>
        <expression>{!txtProfitCenterCode}+&quot; &quot;+{!txtOrganization}+&quot; &quot;+{!txtProfitCenterName}</expression>
    </formulas>
    <formulas>
        <name>fxRoleName</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE({!txtUserProfile},&quot; &quot;, &quot;_&quot;)</expression>
    </formulas>
