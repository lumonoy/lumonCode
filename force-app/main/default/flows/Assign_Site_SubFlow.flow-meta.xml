    <description>Flow to Set or Create Site Record
02.12.2023: Save Active Version as R2</description>
    <environments>Default</environments>
    <formulas>
        <name>fxExistingSiteAddress</name>
        <dataType>String</dataType>
        <expression>IF(NOT(ISBLANK({!recExistingSite.Site_Address__Street__s})),{!recExistingSite.Site_Address__Street__s}+&quot;, &quot;,&quot;&quot;)
+ IF(NOT(ISBLANK({!recExistingSite.Site_Address__PostalCode__s})),{!recExistingSite.Site_Address__PostalCode__s}+&quot;, &quot;,&quot;&quot;)
+ IF(NOT(ISBLANK({!recExistingSite.Site_Address__City__s})),{!recExistingSite.Site_Address__City__s},&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>fxNewSiteAddress</name>
        <dataType>String</dataType>
        <expression>IF(NOT(ISBLANK({!recSite.Site_Address__Street__s})),{!recSite.Site_Address__Street__s}+&quot;, &quot;,&quot;&quot;)
+ IF(NOT(ISBLANK({!recSite.Site_Address__PostalCode__s})),{!recSite.Site_Address__PostalCode__s}+&quot;, &quot;,&quot;&quot;)
+ IF(NOT(ISBLANK({!recSite.Site_Address__City__s})),{!recSite.Site_Address__City__s},&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>fxOtherDeliveryAddress</name>
        <dataType>String</dataType>
        <expression>TEXT({!recOpportunity.Delivery_Address__CountryCode__s}) + TEXT({!recOpportunity.Delivery_Address__StateCode__s})
+ {!recOpportunity.Delivery_Address__City__s}+ {!recOpportunity.Delivery_Address__PostalCode__s}
+ {!recOpportunity.Delivery_Address__Street__s}
+ TEXT({!recOpportunity.Delivery_Address__Latitude__s} )+ TEXT({!recOpportunity.Delivery_Address__Longitude__s})</expression>
    </formulas>
    <formulas>
        <name>fxOtherInstallationAddress</name>
        <dataType>String</dataType>
        <expression>TEXT({!recOpportunity.Installation_Address__CountryCode__s}) + TEXT({!recOpportunity.Installation_Address__StateCode__s})
+ {!recOpportunity.Installation_Address__City__s}+ {!recOpportunity.Installation_Address__PostalCode__s}
+ {!recOpportunity.Installation_Address__Street__s}
+ TEXT({!recOpportunity.Installation_Address__Latitude__s} )+ TEXT({!recOpportunity.Installation_Address__Longitude__s})</expression>
    </formulas>
