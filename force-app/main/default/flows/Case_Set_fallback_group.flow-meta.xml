        </defaultConnector>
        <defaultConnectorLabel>Group not set</defaultConnectorLabel>
        <rules>
            <name>Group_is_Set</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.CaseGroup__c</leftValueReference>
                <operator>IsBlank</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <label>Group is Set</label>
        </rules>
    </decisions>
    <decisions>
