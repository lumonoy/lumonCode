            <conditions>
                <leftValueReference>OwnerType</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Queue</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Queue_Default_as_FB_Group</targetReference>
            </connector>
            <label>Queue Owned</label>
        </rules>
    </decisions>
    <decisions>
