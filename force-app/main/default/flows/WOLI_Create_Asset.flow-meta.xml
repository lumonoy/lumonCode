    <decisions>
        <description>If there is no configuration plan, then we should not create Asset either.</description>
        <name>Configuration_Plan_exists</name>
        <label>Configuration Plan exists</label>
        <locationX>931</locationX>
        <locationY>595</locationY>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Configuration_Plan.Id</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Plan_Type</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <description>If the Configuration Plan Type is one of these and there has been installation for one of these, then the Asset creation needs to be suppressed.</description>
        <name>Plan_Type</name>
        <label>Plan Type</label>
        <locationX>1013</locationX>
        <locationY>716</locationY>
        <defaultConnector>
            <targetReference>Type_of_Asset_to_create</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Glazings or Blinds</defaultConnectorLabel>
        <rules>
            <name>Non_Asset_creation_Types</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Configuration_Plan.Plan_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Services</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Configuration_Plan.Plan_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Sales</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Configuration_Plan.Plan_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Online</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Configuration_Plan.Plan_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Extra Sales</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Configuration_Plan.Plan_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Supplies</stringValue>
                </rightValue>
            </conditions>
            <label>Non-Asset creation Types</label>
        </rules>
    </decisions>
