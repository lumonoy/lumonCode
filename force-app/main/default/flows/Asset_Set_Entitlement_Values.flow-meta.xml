        </defaultConnector>
        <defaultConnectorLabel>No Warranty</defaultConnectorLabel>
        <rules>
            <name>Warranty</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Entitlement</leftValueReference>
