    <description>Trigger Flow to Set the Entitlement (Warranty) for a New Asset</description>
    <environments>Default</environments>
    <formulas>
        <name>InstallDatePlus5Years</name>
        <dataType>Date</dataType>
        <expression>IF(
AND(
MONTH(DateValue({!$Record.CreatedDate})) = 2,
DAY(DateValue({!$Record.CreatedDate})) = 29
),
DATE(
YEAR(DateValue({!$Record.CreatedDate}))+5,
3,
1),
DATE(
YEAR(DateValue({!$Record.CreatedDate}))+5,
MONTH(DateValue({!$Record.CreatedDate})),
DAY(DateValue({!$Record.CreatedDate}))
)
)</expression>
    </formulas>
