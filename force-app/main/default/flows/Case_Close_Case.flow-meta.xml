    <decisions>
        <name>Has_Pictures</name>
        <label>Has Pictures?</label>
        <locationX>336</locationX>
        <locationY>1058</locationY>
        <defaultConnector>
            <targetReference>ErrorNoPics</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No Pictures</defaultConnectorLabel>
        <rules>
            <name>Pictures</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Case.hasPictures__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Case_Resolution</targetReference>
            </connector>
            <label>Pictures</label>
        </rules>
    </decisions>
