    <decisions>
        <name>is_Supported_Case</name>
        <label>is Supported Case?</label>
        <locationX>781</locationX>
        <locationY>842</locationY>
        <defaultConnector>
            <targetReference>ErrorScreenNonSupportedCase</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Other Case</defaultConnectorLabel>
        <rules>
            <name>Supported_Case</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Case</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>User_is_Case_owner</targetReference>
            </connector>
            <label>Supported Case</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_Supported_RecordType</name>
        <label>Is Supported RecordType?</label>
        <locationX>957</locationX>
        <locationY>350</locationY>
        <defaultConnector>
            <targetReference>Add_RecordTypeId</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Other Record Type</defaultConnectorLabel>
        <rules>
            <name>Supported_Record_Type</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Loop_RecordTypes.DeveloperName</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>RECL</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Loop_RecordTypes.DeveloperName</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>INST</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Loop_RecordTypes.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>SPEC</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Add_RecordTypeId</targetReference>
            </connector>
            <label>Supported Record Type</label>
        </rules>
    </decisions>
    <decisions>
        <name>User_is_Case_owner</name>
        <label>User is Case owner?</label>
        <locationX>545</locationX>
        <locationY>950</locationY>
        <defaultConnector>
            <targetReference>ErrorScreenNotOwner</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes2</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Get_Case.OwnerId</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>fxLongUserId</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Profile.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>System Administrator</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Has_Pictures</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
