<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>hasSpecialProduct__c</fullName>
    <description>Indicates if there configuration Item refers to a Product that requires Manual Processing or needs special handling</description>
    <externalId>false</externalId>
    <formula>IF((Product__r.Special__c ||  Product__r.isManualProcessing__c), true, false)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>hasSpecialProduct</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
