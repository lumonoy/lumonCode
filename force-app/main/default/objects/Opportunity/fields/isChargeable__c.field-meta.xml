<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>isChargeable__c</fullName>
    <description>Indicates if Opportunity is chargeable</description>
    <externalId>false</externalId>
    <formula>if (isReclamation__c &amp;&amp;  
      (Main_Project_Number__c =  Sub_Project_Number__c 
        || Sub_Project_Code__c = 1),true,false)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>isChargeable</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
