<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contract_Signed__c</fullName>
    <description>This field is checked when associated final contract is signed</description>
    <externalId>false</externalId>
    <formula>ISPICKVAL(Contract.Status, &quot;Signed&quot;) || ISPICKVAL(Contract.Status,&quot;Activated&quot;) &amp;&amp;  ISPICKVAL(Contract.Contract_type__c, &quot;Final Contract&quot;)</formula>
    <label>Contract Signed</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
