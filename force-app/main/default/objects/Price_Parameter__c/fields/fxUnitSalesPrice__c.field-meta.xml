<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>fxUnitSalesPrice__c</fullName>
    <description>Sales Price of an Item per Unit</description>
    <externalId>false</externalId>
    <formula>(IF ( Min_Sales_Price__c &gt;0 ,
         Min_Sales_Price__c + (Min_Sales_Price__c *( Price_Multiplier__c -1)* (Transmission__c/100)),
         Base_Price__c + (Base_Price__c *( Price_Multiplier__c -1)* (Transmission__c/100))))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>fxUnitSalesPrice</label>
    <precision>18</precision>
    <required>false</required>
    <scale>7</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
