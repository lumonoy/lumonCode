<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CaseInQueue__c</fullName>
    <description>Indicates is Case is Owned by a Queue</description>
    <externalId>false</externalId>
    <formula>IF(ISBLANK(Owner:Queue.Id),FALSE,TRUE)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Indicates is Case is Owned by a Queue</inlineHelpText>
    <label>Case In Queue</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
