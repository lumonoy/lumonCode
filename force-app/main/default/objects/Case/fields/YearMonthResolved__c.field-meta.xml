<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>YearMonthResolved__c</fullName>
    <description>Indicates the Year Month when Case is Resolved</description>
    <externalId>false</externalId>
    <formula>TEXT(YEAR(DATEVALUE( Nullvalue(CaseResolvedDate__c, ClosedDate) )))
+&quot;-&quot;
+LPAD(TEXT(MONTH(DATEVALUE(Nullvalue(CaseResolvedDate__c, ClosedDate)))),2,&quot;0&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>YearMonth Resolved</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
