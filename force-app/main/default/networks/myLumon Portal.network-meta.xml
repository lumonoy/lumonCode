    <site>myLumon_Portal</site>
    <status>Live</status>
    <tabs>
        <defaultTab>home</defaultTab>
        <standardTab>Chatter</standardTab>
    </tabs>
    <welcomeTemplate>myLumonPortal/LP_NewPortalUser1</welcomeTemplate>
